        <!-- CONTAINER LOAD PLAN -->
        <record id="view_dmpi_crm_clp_search" model="ir.ui.view">
            <field name="name">dmpi.crm.clp.search</field>
            <field name="model">dmpi.crm.clp</field>
            <field name="arch" type="xml">
                <search>
                    <field name="container_no"/>
                    <field name="control_no"/>
                    <field name="dr_id"/>
                    <field name="customer"/>

                    <separator/>

                    <filter name="sap_generated" string="Sap Generated" domain="[('status', '=', 'sap_generated')]"/>
                    <filter name="qa_confirmed" string="QA Confirmed" domain="[('status', '=', 'qa_confirmed')]"/>
                    <filter name="preship_generated" string="Pre-shipment Generated" domain="[('status', '=', 'preship_generated')]"/>
                    <filter name="preship_confirmed" string="Pre-shipment Confimred" domain="[('status', '=', 'preship_confirmed')]"/>
                </search>
            </field>
        </record>

        <record id="view_dmpi_crm_clp_tree" model="ir.ui.view">
            <field name="name">dmpi.crm.clp.tree</field>
            <field name="model">dmpi.crm.clp</field>
            <field name="priority">1</field>
            <field name="arch" type="xml">
                <tree string="Container Load Plan">
                    <field name="control_no"/>
                    <field name="dr_id"/>
                    <field name="container_no"/>
                    <field name="plant"/>
                    <field name="port_origin"/>
                    <field name="port_destination"/>
                    <field name="customer"/>
                    <field name="boxes" sum="Total Boxes"/>
                    <field name="week"/>
                    <field name="status"/>
                </tree>
            </field>
        </record>

        <record id="view_dmpi_crm_clp_form" model="ir.ui.view">
            <field name="name">dmpi.crm.clp.form</field>
            <field name="model">dmpi.crm.clp</field>
            <field name="arch" type="xml">
                <form string="Container Load Plan">
                    <header>
                        <button name="print_clp" string="Print Container Load Plan (Internal)" type="object" class="oe_highlight" attrs="{'invisible':[('status','=','sap_generated')]}"/>

                        <button name="print_clp_customer" string="Print Container Load Plan" type="object" class="oe_highlight" attrs="{'invisible':[('status','=','sap_generated')]}"/>

                        <button name="action_confirm_clp" string="Confirm" type="object" class="oe_highlight" attrs="{'invisible':[('status','!=','sap_generated')]}" confirm="Are you sure you want to confirm the CLP?" groups="dmpi_crm.group_custom_crm_qa_staff"/>
                        
                        <button name="action_generate_preship" string="Generate Pre-Shipment" type="object" class="oe_highlight" attrs="{'invisible':[('status','!=','qa_confirmed')]}"/>

                        <field name="status" widget="statusbar" statusbar_visible="sap_generated,qa_confirmed,preship_generated,preship_confirmed"/>
                    </header>
                    <sheet>
                        <h1><field name="dr_no" readonly="1"/>/<field name="control_no" attrs="{'readonly':True}"/></h1>
                        <group>
                            <group>
                                <field name="dr_id" invisible="1"/>
                                <field name="container_no"/>
                                <field name="seal_no"/>
                                <field name="vessel_name"/>
                                <field name="plant"/>
                                <field name="port_origin"/>
                                <field name="port_destination"/>
                                <field name="customer"/>
                            </group>
                            <group>
                                <field name="week"/>
                                <field name="brand"/>
                                <field name="shell_color"/>
                                <field name="description"/>
                                <field name="boxes"/>
                                <field name="installed" string="Installed Already?"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Packages">
                                <field name="clp_line_ids">
                                    <tree string="Packages" editable="bottom">
                                        <field name="position"/>
                                        <field name="tag_no"/>
                                        <field name="pack_code"/>
                                        <field name="product_crown"/>
                                        <field name="qty"/>
                                        <field name="pack_size"/>
                                    </tree>
                                </field>
                            </page>
                            <page string="Others">
                                <group>
                                    <group>
                                        <h3><strong style="color:#79717D;">Loading Date/Time</strong></h3>
                                        <field name="date_start"/>
                                        <field name="date_end"/>
                                        <field name="date_depart"/>
                                        <field name="date_arrive"/>

                                        <h3><strong style="color:#79717D;">Summary of Cases</strong></h3>
<!--                                         <field name="summary_case_a" string="A"/>
                                        <field name="summary_case_b" string="B"/>
                                        <field name="summary_case_c" string="C"/> -->
