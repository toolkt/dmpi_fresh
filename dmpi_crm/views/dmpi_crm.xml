<?xml version="1.0" encoding="UTF-8"?>
<odoo>
        

        <record id="view_dmpi_crm_partner_search" model="ir.ui.view">
            <field name="name">Partner</field>
            <field name="model">dmpi.crm.partner</field>
            <field name="arch" type="xml">
                <search>
                    <field name="name"/>
                    <field name="customer_code"/>
                    <filter name="filter_active" string="Active" domain="[('active', '=', True)]"/>
                    <group>
                        <filter string="Active" context="{'group_by': 'active'}"/>
                    </group>
                </search>
            </field>
        </record>

        <record id="view_dmpi_crm_partner_tree" model="ir.ui.view">
            <field name="name">Partner</field>
            <field name="model">dmpi.crm.partner</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="short_name"/>
                    <field name="customer_code" />
                    <field name="sales_org" />
                    <field name="dist_channel" />
                    <field name="division" />
                    <field name="default_plant" />
                    <field name="country" />
                    <field name="country_code" />
                    <field name="active" />
                    <field name="tag_ids" widget="many2many_tags"/>
                </tree>
            </field>
        </record>


        <record id="view_dmpi_crm_partner_form" model="ir.ui.view">
            <field name="name">Partner</field>
            <field name="model">dmpi.crm.partner</field>
            <field name="arch" type="xml">
                <form class="o_form_document">
                    <sheet>
                        <h1><field name="name"/></h1>
                        <label for="active"/><field name="active"/>
                        <group>
                            <group>
                                <field name="short_name"/>
                                <field name="customer_code"/>
                                <field name="default_plant"/>
                                <field name="account_group" />
                                <field name="default_validity" />
                                <field name="tag_ids" widget="many2many_tags"/>
                            </group>
                            <group string="Sales Area">
                                <field name="sales_org"/>
                                <field name="dist_channel"/>
                                <field name="division"/>
                            </group>
                            <group string="Address 1">
                                <field name="street"/>
                                <field name="city"/>
                                <field name="city_code"/>
                                <field name="postal_code"/>
                                <field name="country"/>
                                <field name="country_code" readonly="1"/>
                                <field name="email"/>
                                <field name="phone"/>
                                <field name="fax"/>
                            </group>
                            <group string="Address 2">
                                <field name="address"/>
                                <field name="contact"/>
                                <field name="function_ids" options="{'no_quick_create':True,'no_create_edit':True}" widget="many2many_tags"/>
                            </group>
                        </group>

                        <notebook>
                            <page string="Ship To">
                                <field name="ship_to_ids">
                                    <tree>
                                        <field name="name"/>
                                        <field name="customer_code"/>
                                        <field name="country"/>
                                    </tree>
                                </field>
                            </page>
                            <page string="Notify Party">
                                <field name="notify_ids">
                                    <tree>
                                        <field name="name"/>
                                        <field name="customer_code"/>
                                        <field name="country"/>
                                    </tree>
                                </field>
                            </page>
                            <page string="Mailing Addresses">
                                <field name="mailing_ids">
                                    <tree>
                                        <field name="name"/>
                                        <field name="customer_code"/>
                                        <field name="country"/>
                                    </tree>
                                </field>
                            </page>
                            <page string="Assigned Products">
                                <field name="product_ids">
                                    <tree>
                                        <field name="code_id"/>
                                        <field name="sku"/>
                                        <field name="product_class"/>
                                        <field name="packaging"/>
                                        <field name="variant"/>
                                    </tree>
                                </field>
                            </page>
                            <page string="Accounts Receivables">
                                <field name="ar_records" />
                            </page>
                        </notebook>

                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>

