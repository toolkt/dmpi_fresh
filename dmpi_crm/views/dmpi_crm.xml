        <record id="view_dmpi_crm_product_price_list_form" model="ir.ui.view">
            <field name="name">Product Price List</field>
            <field name="model">dmpi.crm.product.price.list</field>
            <field name="arch" type="xml">
                <form class="o_form_document">
                    <sheet>
                        <div class="oe_button_box" name="button_box">
                            <button name="toggle_active" type="object" class="oe_stat_button" icon="fa-check">
                                <field name="active" widget="boolean_button" options="{&quot;terminology&quot;: &quot;active&quot;}"/>
                            </button>
                            <button name="download_pricelist_template" type="object" class="oe_stat_button" icon="fa-download" string="Download Template"/>
                        </div>
                        <h1><field name="name"/></h1>
                        <group>
                            <group>
                                <field name="date"/>
                                <field name="week_no"/>
                            </group>
                            <group>
                                <field name="upload_filename" invisible="1"/>
                                <field name="upload_file" filename="upload_filename"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Items">
                                <field name="item_ids">
                                    <form>
                                        <group>
                                            <group>
                                                <field name="partner_id" options="{'no_quick_create':True,'no_create_edit':True}" required="1"/>
                                                <field name="customer_code" invisible="1"/>
                                                <field name="sales_org"/>
                                                <field name="freight_term"/>
                                                <field name="remarks" invisible="1"/>
                                                <field name="tag_ids" widget="many2many_tags" options="{'no_quick_create':True,'no_create_edit':True}"/>
                                            </group>
                                            <group>
                                                <field name="product_id" options="{'no_quick_create':True,'no_create_edit':True}" required="1"/>
                                                <field name="material" invisible="1"/>
                                                <field name="amount" required="1"/>
                                                <field name="currency"/>
                                                <field name="uom" required="1"/>
                                                <field name="valid_from" required="1"/>
                                                <field name="valid_to" required="1"/>
                                            </group>
                                        </group>
                                    </form>
                                    <tree>
                                        <field name="product_id"/>
                                        <field name="material"/>
                                        <field name="sales_org" invisible="1"/>
                                        <field name="customer_code" invisible="1"/>
                                        <field name="partner_id"/>
                                        <field name="freight_term" invisible="1"/>
                                        <field name="amount"/>
                                        <field name="currency" invisible="1"/>
                                        <field name="uom"/>
                                        <field name="valid_from"/>
                                        <field name="valid_to"/>
                                        <field name="remarks" invisible="1"/>
                                        <field name="tag_ids" widget="many2many_tags" options="{'no_quick_create':True,'no_create_edit':True}"/>
                                    </tree>
                                </field>
                            </page>
                            
                            <page string="Instructions">
                                <ol>
                                    <li>
                                        Pricelist is in "inactive" state upon creation. Activate pricelist to use it
                                    </li>
                                    <li>
                                        Upload a CSV file to populate items in the pricelist. The CSV template can also be downloaded using one of the action buttons.
                                    </li>
                                    <li>
                                        The uploaded file can be downloaded then re-upload again to update the pricelist with the changes.
                                    </li>
                                    <li>
                                        To create new price tags, go to the Product Price Tag menu.
                                    </li>
                                    <li>
                                        For week related price tags, the date from and date to fields are required.
                                    </li>
                                </ol>
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>


